.promo {
  position: relative;
  margin-top: -111px;
  margin-bottom: 80px;
  overflow: hidden;
}
.promo-slide-controls {
  position: absolute;
  right: 83px;
  bottom: 135px;
  z-index: 1;
}
.promo label {
  display: inline-block;
  min-width: 23px;
  min-height: 23px;
  border-radius: 50%;
  background-image: url(../img/slider-button-empty.svg);
  background-repeat: no-repeat;
  background-size: 12px 12px;
  background-position: 50% 50%;
  cursor: pointer;
}
.promo-slide-btn:nth-of-type(1):checked ~ .promo-slide-controls :nth-child(1) {
  background-image: url(../img/slider-button.svg);
}
.promo-slide-btn:nth-of-type(1):focus ~ .promo-slide-controls :nth-child(1) {
  outline: auto;
}
.promo-slide-btn:nth-of-type(2):focus ~ .promo-slide-controls :nth-child(2) {
  outline: auto;
}
.promo-slide-btn:nth-of-type(3):focus ~ .promo-slide-controls :nth-child(3) {
  outline: auto;
}
.promo-slide-btn:nth-of-type(2):checked ~ .promo-slide-controls :nth-child(2) {
  background-image: url(../img/slider-button.svg);
}
.promo-slide-btn:nth-of-type(3):checked ~ .promo-slide-controls :nth-child(3) {
  background-image: url(../img/slider-button.svg);
}
.promo-list {
  display: flex;
  margin: 0;
  padding: 0;
  list-style: none;
  transition: transform 0.8s ease;
}
.promo-list .promo-item {
  display: flex;
  width: 1160px;
  flex-shrink: 0;
  visibility: hidden;
  transition: visibility 0.8s ease;
}
.promo-slide-btn:nth-of-type(1):checked ~ .promo-list {
  transform: translateX(0px);
}
.promo-slide-btn:nth-of-type(2):checked ~ .promo-list {
  transform: translateX(-1160px);
}
.promo-slide-btn:nth-of-type(3):checked ~ .promo-list {
  transform: translateX(-2320px);
}
.promo-slide-btn:nth-of-type(1):checked ~ .promo-list > :nth-child(1) {
  visibility: visible;
}
.promo-slide-btn:nth-of-type(2):checked ~ .promo-list > :nth-child(2) {
  visibility: visible;
}
.promo-slide-btn:nth-of-type(3):checked ~ .promo-list > :nth-child(3) {
  visibility: visible;
}
