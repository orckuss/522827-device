@font-face {
  font-family: "Gilroy";
  font-weight: 300;
  font-style: normal;
  src: 
    local("Gilroy Light"),
    local("Gilroy-Light"),
    url("../fonts/gilroylight.woff2") format("woff2");
}
@font-face {
  font-family: "Gilroy";
  font-weight: 800;
  font-style: normal;
  src: 
    local("Gilroy-ExtraBold"),
    local("Gilroy ExtraBold"),
    url("../fonts/gilroyextrabold.woff2") format("woff2");
}
html { box-sizing: border-box; }
*, *::before, *::after { box-sizing: inherit; }
body {
  min-width: 1200px;
  padding: 0;
  margin: 0;
  font-family: "Open Sans", "Arial", sans-serif;
  font-weight: 300;
  font-size: 15px;
  line-height: 30px;
  letter-spacing: 0.01em;
  color: rgba(70, 70, 70, 1.00);
}
a {
  text-decoration: none;
}
img {
  max-width: 100%;
  height: auto;
}
.container {
  width: 1160px;
  margin: 0px auto;
}
.visual-hidden:not(:focus):not(:active),
input[type="checkbox"].visual-hidden,
input[type="radio"].visual-hidden {
  position: absolute;
	height: 1px;
	width: 1px;
	margin: -1px;
	padding: 0;
	border: 0;
  white-space: nowrap;
	overflow: hidden;
	clip: rect(0 0 0 0);
}
.btn {
  position: relative;
  z-index: 1;
  display: inline-block;
  min-width: 160px; 
  max-width: 100%;
  padding: 11px;
  font-family: "Gilroy", "Arial", sans-serif;
  font-weight: 800;
  font-size: 18px;
  line-height: 1;
  letter-spacing: 0.2em;
  color: rgba(0, 0, 0, 1.00);
  text-transform: uppercase;
  text-align: center;
  vertical-align: top;
  background: 0;
  border: none;
  cursor: pointer;
}
.btn::after {
  content: "";
  position: absolute;
  top: 50%;
  transform: translateY(-50%);   
  left: 0%;
  z-index: -1;
  width: 100%;
  height: 8px;
  background-color: #f0c52e;
  transition: .3s linear;
}
.btn:hover::after,
.btn:focus::after {
  height: 100%;
}
.btn:hover,
.btn:focus {
  outline: none;
}
.btn:active {
  color: rgba(0, 0, 0, 0.30);
}
.header-logo[href="index.html"]:hover,
.header-logo[href="index.html"]:focus,
.footer-logo[href="index.html"]:hover,
.footer-logo[href="index.html"]:focus {
  opacity: 0.6;
  outline: none;
}
.header-logo[href="index.html"]:active,
.footer-logo[href="index.html"]:active {
  opacity: 0.3;
}
.page-header {
