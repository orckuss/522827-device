.modal {
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 2;
  display: none;
  width: 960px;
  transform: translate(-50%, -50%);
  background-color: rgba(255, 255, 255, 1.00);
  box-shadow: 0 10px 20px rgba(4, 6, 6, 0.20);
}
.modal-close {
  position: absolute;
  top: 22px;
  right: 22px;
  width: 55px;
  height: 55px;
  border: none;
  background-color: rgba(240, 197, 46, 0.50);
  border-radius: 50%;
  outline: none;
}
.modal-close::before,
.modal-close::after {
  content: "";
  position: absolute;
  top: 50%;
  left: 50%;
  width: 32px;
  height: 2px;
  margin-left: -16px;
  margin-top: -1px;
  background-color: rgba(255, 255, 255, 1.00);
  transform: rotate(45deg);
}
.modal-close::after {
  transform: rotate(-45deg);
}
.modal-close:hover,
.modal-close:focus {
  background-color: rgba(240, 197, 46, 1.00);
}
.modal-close:active {
  background-color: rgba(240, 197, 46, 0.30);
}
.modal-show {
  display: block;
}
.modal-feedback {
  padding: 0 100px;
  padding-top: 96px;
  padding-bottom: 78px;
  box-shadow: 0 20px 40px rgba(4, 6, 6, 0.75);
}
.feedback {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.feedback p {
  width: 360px;
  margin: 0;
  margin-bottom: 35px;
}
.feedback .feedback-message {
  width: 100%;
  margin-bottom: 42px;
}
.feedback-message textarea {
  height: 155px;
}
.feedback label {
  display: block;
  padding-bottom: 11px;
  font-family: "Gilroy", "Arial", sans-serif;
  font-weight: 800;
  font-size: 18px;
  line-height: 18px;
  letter-spacing: 0;
  color: rgba(0, 0, 0, 1.00);
}
.feedback input,
.feedback textarea {
  width: 100%;
  padding: 12px 17px;
  margin: 0;
  font-family: inherit;
  font-weight: 400;
  font-size: 14px;
  line-height: 18px;
  letter-spacing: 0;
  color: rgba(70, 70, 70, 0.40);
  border: 3px solid transparent;
  background-color: rgba(229, 229, 229, 0.50);
  outline: none;
}
.feedback input:invalid {
  background-color: rgba(237, 181, 181, 0.50);
}
.feedback input:hover,
.feedback textarea:hover {
  background-color: rgba(229, 229, 229, 0.80);
