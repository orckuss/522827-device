  padding-left: 40px;
  font: inherit;
  line-height: 36px;
  letter-spacing: 0.01em;
  color: rgba(0, 0, 0, 1.00);
  border: none;
  background-color: transparent;
  opacity: 0.4;
}
.search-form::before {
  content: "";
  position: absolute;
  left: 3px;
  bottom: 16px;
  width: 16px;
  height: 16px;
  background-image: url(../img/search.svg);
  background-repeat: no-repeat;
}
.search-form input:hover {
  opacity: 0.8;
}
.search-form input:focus {
  outline: none;
  opacity: 1;
  border-bottom: 2px solid rgba(0, 0, 0, 1.00);
}
.search-hidden {
  padding: 5px 19px;
  font: inherit;
  line-height: 36px;
  letter-spacing: 0.01em;
  color: rgba(0, 0, 0, 1.00);
  border: none;
  border: 2px solid rgba(0, 0, 0, 1.00);
  background-color: transparent;
  opacity: 0;
/*  visibility: hidden;*/
}
.search-form input:focus ~ .search-hidden,
.search-hidden:focus {
  opacity: 1;
/*  visibility: visible;*/
}
.search-hidden:hover,
.search-hidden:focus {
  color: rgba(255, 255, 255, 1.00);
  background-color: rgba(0, 0, 0, 1.00);
  outline: none;
/*  visibility: visible;*/
}
.search-hidden:active {
  color: rgba(255, 255, 255, 0.30);
}
